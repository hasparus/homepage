{
  "name": "@hasparus/homepage",
  "private": true,
  "description": "My personal abode on the world wide web",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md}\"",
    "start": "npm run develop",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/gatsby/dist/bin/gatsby develop\"",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.{js,jsx,tsx,ts}",
    "codegen": "graphql-codegen --config ./gql-global-codegen.yml"
  },
  "dependencies": {
    "@emotion/core": "^10.0.17",
    "@mdx-js/mdx": "^1.5.0",
    "@mdx-js/react": "^1.5.0",
    "@types/lodash": "^4.14.141",
    "@types/react-dom": "^16.9.1",
    "@types/react-helmet": "^5.0.11",
    "@types/theme-ui": "^0.2.3",
    "fp-ts": "^2.1.0",
    "gatsby": "^2.15.28",
    "gatsby-plugin-codegen": "^1.0.4",
    "gatsby-plugin-manifest": "^2.2.20",
    "gatsby-plugin-mdx": "^1.0.46",
    "gatsby-plugin-react-helmet": "^3.1.10",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.2.27",
    "gatsby-plugin-theme-ui": "^0.2.43",
    "gatsby-plugin-typescript": "^2.1.11",
    "gatsby-remark-copy-linked-files": "^2.1.23",
    "gatsby-remark-images": "^3.1.25",
    "gatsby-remark-smartypants": "^2.1.11",
    "gatsby-remark-vscode": "^1.2.1",
    "gatsby-source-filesystem": "^2.1.28",
    "gatsby-transformer-sharp": "^2.2.19",
    "lodash": "^4.17.15",
    "polished": "^3.4.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-helmet": "^5.2.1",
    "remark-slug": "^5.1.2",
    "theme-ui": "^0.2.43",
    "typeface-open-sans": "^0.0.75",
    "typeface-passion-one": "^0.0.72",
    "typescript": "^3.7.0-dev.20191004",
    "utility-types": "^3.8.0",
    "webpack-notifier": "^1.8.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.8.1",
    "@graphql-codegen/typescript": "^1.8.1",
    "eslint": "^6.6.0",
    "eslint-config-chop-chop": "^1.2.1",
    "husky": "^3.0.9",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hasparus/homepage"
  },
  "bugs": {
    "url": "https://github.com/hasparus/homepage"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn typecheck && yarn lint"
    }
  }
}
